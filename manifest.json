{
  "manifest_version": 2,
  "name": "patty",
  "version": "0.12",
  "description": "Fast traslate for you",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "lib/jquery-3.4.1.min.js","background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icons/v2_48.png"
    }
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["lib/jquery-3.4.1.min.js","content.js"]
    }
  ],
  "web_accessible_resources": [
    "script.js",
    "icons/*"
  ]
}